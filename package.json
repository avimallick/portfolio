{
  "name": "dual-activity-service",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --env-file=.env --experimental-strip-types scripts/dev.ts",
    "check": "tsc --noEmit",
    "build": "tsc --noEmit",
    "vercel:dev": "npx vercel dev",
    "vercel:build": "npx vercel build",
    "vercel:deploy": "npx vercel deploy --prebuilt --prod",
    "deploy": "npm run check && npm run vercel:build && npm run vercel:deploy"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.7.3"
  }
}
